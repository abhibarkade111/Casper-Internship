<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/iphone_11_p"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:clipToOutline="true"
    tools:context=".EmergencyContactActivity"
    tools:targetApi="s">


    <com.google.android.material.card.MaterialCardView
        android:id="@+id/card1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="10dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="10dp"
        android:outlineAmbientShadowColor="@color/cardview_dark_background"
        android:clickable="true"
        android:focusable="true"
        app:cardBackgroundColor="@color/primaryLightColor">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="10dp">

            <TextView
                android:id="@+id/nen"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="National Emergency Number"
                android:textColor="@color/secondaryTextColor"
                android:textSize="20sp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/nen"
                android:text="112"
                android:textColor="@color/secondaryTextColor"
                android:textSize="18dp" />
        </RelativeLayout>
    </com.google.android.material.card.MaterialCardView>

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/card2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="10dp"
        android:layout_margin="10dp"
        android:layout_below="@id/card1"
        android:outlineAmbientShadowColor="@color/cardview_dark_background"
        android:clickable="true"
        android:focusable="true"
        app:cardBackgroundColor="@color/primaryLightColor">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="10dp">

            <TextView
                android:id="@+id/police"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Police"
                android:textColor="@color/secondaryTextColor"
                android:textSize="20sp" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/police"
                android:text="100"
                android:textSize="18sp"
                android:textColor="@color/secondaryTextColor"
                />
        </RelativeLayout>
    </com.google.android.material.card.MaterialCardView>

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/card3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="10dp"
        android:layout_margin="10dp"
        android:layout_below="@id/card2"
        android:outlineAmbientShadowColor="@color/cardview_dark_background"
        android:clickable="true"
        android:focusable="true"
        app:cardBackgroundColor="@color/primaryLightColor">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="10dp">
            <TextView
                android:id="@+id/fire"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:text="Fire"
                android:textColor="@color/secondaryTextColor"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/fire"
                android:text="101"
                android:textSize="18sp"
                android:textColor="@color/secondaryTextColor"
                />
        </RelativeLayout>
    </com.google.android.material.card.MaterialCardView>
    <com.google.android.material.card.MaterialCardView
        android:id="@+id/card4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="10dp"
        android:layout_margin="10dp"
        android:layout_below="@id/card3"
        android:outlineAmbientShadowColor="@color/cardview_dark_background"
        android:clickable="true"
        android:focusable="true"
        app:cardBackgroundColor="@color/primaryLightColor">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="10dp">
            <TextView
                android:id="@+id/ambulance"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:text="Ambulance"
                android:textColor="@color/secondaryTextColor"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/ambulance"
                android:text="102"
                android:textSize="18sp"
                android:textColor="@color/secondaryTextColor"
                />
        </RelativeLayout>
    </com.google.android.material.card.MaterialCardView>
    <com.google.android.material.card.MaterialCardView
        android:id="@+id/card5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="10dp"
        android:layout_margin="10dp"
        android:layout_below="@id/card4"
        android:outlineAmbientShadowColor="@color/cardview_dark_background"
        android:clickable="true"
        android:focusable="true"
        app:cardBackgroundColor="@color/primaryLightColor">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="10dp">
            <TextView
                android:id="@+id/women_helpline"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:text="Women Helpline"
                android:textColor="@color/secondaryTextColor"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/women_helpline"
                android:text="1091"
                android:textSize="18sp"
                android:textColor="@color/secondaryTextColor"
                />
        </RelativeLayout>
    </com.google.android.material.card.MaterialCardView>
    <com.google.android.material.card.MaterialCardView
        android:id="@+id/card6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:cardCornerRadius="10dp"
        android:layout_margin="10dp"
        android:layout_below="@id/card5"
        android:outlineAmbientShadowColor="@color/cardview_dark_background"
        android:clickable="true"
        android:focusable="true"
        app:cardBackgroundColor="@color/primaryLightColor">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="10dp">
            <TextView
                android:id="@+id/domestic_abuse"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"
                android:text="Women Helpline (Domestic Abuse)"
                android:textColor="@color/secondaryTextColor"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/domestic_abuse"
                android:text="181"
                android:textSize="18sp"
                android:textColor="@color/secondaryTextColor"
                />
        </RelativeLayout>
    </com.google.android.material.card.MaterialCardView>


</RelativeLayout>